<!DOCTYPE html>
<html>
<head>
<script src="http://code.jquery.com/jquery-latest.js"></script>
<script>
$(document).ready(function () {
$("#but1").click(function () {
$("div").fadeIn();
var i=1;
do{
$("div:odd").fadeToggle(2000);
i++;
if(i%2==0){
$("div:even").fadeToggle(2000);}
}
while(i<3000)
$("div:odd").hide();
});

$("#but2").click(function ()
      {
 i=3000;
 $("div").stop($("#but1").click());
 $("div").fadeIn();
 $("div").fadeIn();
});
});
</script>
</head>
<body>
<button id="but1">Click me</button>
<button id="but2">Click </button>

<br><br>
<div style="width:80px;height:80px;display:none;background-color:red;"></div><br>
<div style="width:80px;height:80px;display:none;background-color:blue;"></div>

</body>
</html>
