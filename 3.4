<!DOCTYPE html>
<html>
<head>
<script src="http://code.jquery.com/jquery-latest.js"></script>
<script>

$(document).ready(function () 
{
$("button").click(function () 
   {
   for(var i=0; 10>1;i++){
 $("#div1").fadeIn();
 $("#div2").fadeIn();
 $("#div3").fadeIn();
 $("#div4").fadeIn();
 $("#div5").fadeIn();
 $("#div1").fadeOut(1000);
 $("#div2").fadeOut(3000);
 $("#div3").fadeOut(5000);
 $("#div4").fadeOut(7000);
 $("#div5").fadeOut(9000);

 $("#div1").delay(5000).fadeIn();
 $("#div2").delay(4000).fadeIn();
 $("#div3").delay(3000).fadeIn();
 $("#div4").delay(2000).fadeIn();
$("#div5").delay(1000).fadeIn();
}
   });
   
});
</script>
</head>
<body>
<button>Click me</button> <br><br>
<div id="div1" style="height:40px; width:40px;float:left; background: orange;display:none" ></div>
<div id="div2" style="height:40px; width:40px;float:left; background: blue;display:none" ></div>
<div id="div3" style="height:40px; width:40px;float:left; background: green;display:none" ></div>
<div id="div4" style="height:40px; width:40px;float:left; background: red;display:none " ></div>
<div id="div5" style="height:40px; width:40px;float:left;background: pink;display:none" ></div>

</body>

</html>
